#
# Makefile for "qc1.qup"
#
PROGRAM		= @QPHOME@/bin/qc1.qup

SOURCES		= qc1.ql

BASES		= $(basename $(SOURCES))
OBJECTS		= $(addsuffix .qo,$(BASES))
ASMS =        $(addsuffix .qs,$(BASES))

.PHONY:	all
all: $(PROGRAM)

$(PROGRAM): $(OBJECTS)
	$(COMPILE.qc) -L -o $(PROGRAM) $(OBJECTS)

%.qo: %.qs
	$(COMPILE.qc) -c -o $@ $<

