# Add this file to your ~/.profile

ICMHOME=@ICMHOME@
QPHOME=@QPHOME@
QPPATH=$QPHOME/bin
QPLIBPATH=.
PATH=$QPPATH:$PATH
MANPATH=$QPHOME/doc/man:$MANPATH
TEXINPUTS="$QPHOME/doc/include:$TEXINPUTS"
export QPHOME QPPATH QPLIBPATH PATH TEXINPUTS MANPATH
export PATH=$PATH:$ICMHOME/bin
export LD_LIBRARY_PATH=$LD_LIBRARY_PATH:$ICMHOME/lib
export LD_RUN_PATH=$ICMHOME/lib

